CREATE TABLE usuarios (ID BIGINT NOT NULL, ATIVO TINYINT(1) default 0, CPF VARCHAR(11) NOT NULL UNIQUE, EMAIL VARCHAR(50) NOT NULL, NOME VARCHAR(100) NOT NULL, SENHA VARCHAR(20) NOT NULL, PRIMARY KEY (ID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
